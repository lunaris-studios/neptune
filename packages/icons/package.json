{
  "name": "@nucleus/icons",
  "version": "0.0.1",
  "description": "Nucleus Icon Library",
  "author": "Lunaris Studios",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "typings": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "npm-run-all clean:* --parallel",
    "clean:debug": "rm -rf ./npm-debug*",
    "clean:modules": "rm -rf ./node_modules/",
    "compile": "generate-icons-source",
    "build": "npm run compile && rollup -c",
    "dev": "rollup -cw",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@nucleus/scripts": "^0.0.1",
    "@types/svgo": "^1.3.3",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.27.12",
    "rollup-plugin-typescript2": "^0.25.3",
    "svgo": "^1.3.2",
    "ts-node": "^8.10.1",
    "typescript": "3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:lunaris-studios/nucleus.git",
    "directory": "packages/icons"
  }
}
