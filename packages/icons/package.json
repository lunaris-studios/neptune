{
	"name": "@neptune/icons",
	"version": "0.0.1",
	"description": "Neptune Icon Library",
	"author": "Lunaris Studios",
	"main": "dist/index.js",
	"module": "dist/index.es.js",
	"typings": "dist/src/index.d.ts",
	"files": [
		"dist"
	],
	"types": "dist/index.d.ts",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"clean": "npm-run-all clean:* --parallel",
		"clean:debug": "rm -rf ./npm-debug*",
		"clean:modules": "rm -rf ./node_modules/",
		"compile": "run-p \"compile:*\"",
		"compile:icons": "generate-icons-source",
		"compile:rollup": "rollup -c",
		"dev": "rollup -cw",
		"dist": "run-s: \"dist:*\"",
		"dist:bundle": "rollup -c --environment INCLUDE_DEPS,BUILD:production",
		"lint": "run-p lint:es",
		"lint:es": "es-lint",
		"lint-fix": "es-lint --fix",
		"start": "npm run dev",
		"test": "run-s test:type-check test:jest",
		"test:type-check": "tsc",
		"test:jest": "jest",
		"verify": "npm-run-all compile -p dist test lint"
	},
	"devDependencies": {
		"@neptune/scripts": "^0.0.1",
		"@types/jest": "^25.2.3",
		"@types/svgo": "^1.3.3",
		"jest": "^26.0.1",
		"npm-run-all": "^4.1.5",
		"rollup": "^1.27.12",
		"rollup-plugin-typescript2": "^0.25.3",
		"svgo": "^1.3.2",
		"ts-jest": "^26.0.0",
		"ts-node": "^8.10.1",
		"typescript": "3.8.3"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:lunaris-studios/neptune.git",
		"directory": "packages/icons"
	}
}
