{
  "name": "@nucleus/docs-app",
  "private": true,
  "description": "A website for Nucleus Design System",
  "author": "Lunaris Studios",
  "version": "0.0.1",
  "scripts": {
    "clean": "npm-run-all clean:* --parallel",
    "clean:gatsby": "rm -rf .cache/** && rm -rf public/**",
    "clean:debug": "rm -rf ./npm-debug*",
    "clean:modules": "rm -rf ./node_modules/",
    "build": "gatsby build",
    "dev": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "serve": "gatsby serve",
    "start": "npm run dev"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.5",
    "@mdx-js/react": "^1.5.5",
    "@modulz/primitives": "0.0.1-40",
    "@nucleus/core": "^0.0.1-beta.42",
    "@modulz/radix-icons": "2.4.0",
    "babel-plugin-styled-components": "^1.10.7",
    "gatsby": "^2.19.17",
    "gatsby-plugin-catch-links": "^2.1.25",
    "gatsby-plugin-mdx": "^1.0.72",
    "gatsby-plugin-netlify": "^2.1.32",
    "gatsby-plugin-nprogress": "^2.1.19",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-styled-components": "^3.1.19",
    "gatsby-source-filesystem": "^2.1.48",
    "github-slugger": "^1.2.1",
    "prism-react-renderer": "^1.0.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-live": "^2.2.2",
    "remark-autolink-headings": "^5.2.1",
    "remark-slug": "^5.1.2",
    "styled-components": "^5.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lunaris-studios/nucleus"
  },
  "bugs": {
    "url": "https://github.com/lunaris-studios/nucleus/issues"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
